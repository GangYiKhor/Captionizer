                   <property name="styleSheet">
                    <string notr="true">/* Transcribe */
#frm_transcribe_top &gt; QFrame{
    border: 1px solid transparent;
    border-radius: 15px;
    background-color: #272c36;
}
#frm_transcribe_bottom{
    border: 1px solid transparent;
    border-radius: 15px;
    background-color: #272c36;
    margin: 9px;
}
#frm_transcribe_separator{
    border-radius: 0px;
    background-color: #3d4656;
}

/* Transcribe File Frame */
#lbl_transcribe_title{
    font: bold 14pt 'Segoe UI';
    color: #cccccc;
    padding-left: 1px;
}
#frm_transcribe_file_buttons{
    height: 30px;
}
#lbl_transcribe_file_import, #lbl_transcribe_status{
    font: italic Arial, Helvetica, sans-serif;
}

/* Transcribe Audio Player */
#lbl_transcribe_player_status{
    font: italic bold Arial, Helvetica, sans-serif;
    text-align: center;
    color: #ff3333;
}
#frm_transcribe_player_slider QLabel{
    font-family: Palatino;
}
#sld_transcribe_player::groove{
    border-radius: 1px;
    background: #484f5a;
    height: 2px;
}
#sld_transcribe_player::handle{
    background-color: #007ad9;
    height: 14px;
    width: 14px;
    margin: -7px 0;
    border-radius: 7px;
}
#sld_transcribe_player::handle:disabled{
    background-color: #416988;
}
#sld_transcribe_player::groove:hover{
    background-color: #787878;
}
#sld_transcribe_player::handle:hover{
    background-color: #005cbb;
}
#sld_transcribe_player::sub-page {
    background: #3288df;
}
#frm_transcribe_player_buttons #btn_transcribe_player_play, 
#frm_transcribe_player_buttons #btn_transcribe_player_pause, 
#frm_transcribe_player_buttons #btn_transcribe_player_stop{
    margin: 1px;
    background-color: transparent;
}
#btn_transcribe_player_play:disabled, #btn_transcribe_player_pause:disabled, #btn_transcribe_player_stop:disabled{
    border-radius: 0px;
    background-color: #404c63;
}

/* Transcribe Input */
#frm_transcribe_input QLabel{
    font: 10pt 'Franklin Gothic Medium';
    padding-left: 10px;
    padding-right: 10px;
}
#btn_transcribe_output{
    border: 1px solid #7a7a7a;
    border-radius: 0px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

/* Transcribe Form Transcribe */
#pgr_transcribe_transcribe{
    margin-bottom: -20px;
}
#lbl_transcribe_status{
    font: italic Arial, Helvetica, sans-serif;
    margin-top: -20px;
}
#frm_transcribe_transcribe &gt; QPushButton{
    border: 4px ridge #333d50;
    border-radius: 15px;
}
#frm_transcribe_transcribe &gt; QPushButton:hover{
    border: 4px ridge #3e4c66;
}
#frm_transcribe_transcribe &gt; QPushButton:pressed{
    border: 4px solid #15171a;
}
#btn_transcribe_transcribe{
    font: bold 12pt &quot;Secular One&quot;;
}
#btn_transcribe_cancel{
    font: 'Segoe UI';
}</string>
