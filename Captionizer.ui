               <property name="styleSheet">
                <string notr="true">/* Menubar */
#frm_menubar, #frm_menus{
    background-color: #1b1d23;
    border: none;
}
#frm_menus &gt; QPushButton{
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 19px solid transparent;
    height: 70px;
    width: 140px;
    padding-left: 55px;
    text-align: left;
    font: bold 12pt &quot;Secular One&quot;;
}
#frm_menus &gt; QPushButton:hover{
    background-color: #45464a;
    border-left: 19px solid #45464a;
    border-right: 5px solid #45464a;
}
#frm_menus &gt; QPushButton:pressed{
    background-color: #030406;
    border-left: 19px solid #030406;
    border-right: 5px solid #030406;
}
#frm_menus #btn_menu{
    background-image: url(:/menu/resources/menu/hamburger.png);
    border-bottom: 2px solid #666666;
}
#frm_menus #btn_menuTranscription{
    background-image: url(:/menu/resources/menu/transcribe.png);
}
#frm_menus #btn_menuTranslation{
    background-image: url(:/menu/resources/menu/translate.png);
}</string>
