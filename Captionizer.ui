                   <property name="styleSheet">
                    <string notr="true">/* Translate */
#frm_translate_top &gt; QFrame{
    border: 1px solid transparent;
    border-radius: 15px;
    background-color: #272c36;
}
#frm_translate_bottom{
    border: 1px solid transparent;
    border-radius: 15px;
    background-color: #272c36;
    margin: 9px;
}
#frm_translate_separator{
    border-radius: 0px;
    background-color: #3d4656;
}

/* Translate File Frame */
#lbl_translate_title{
    font:bold 14pt 'Segoe UI';
    color: #cccccc;
    padding-left: 1px;
}
#frm_translate_file_buttons{
    height: 30px;
}

/* Translate Input */
#frm_translate_input QLabel{
    font: 10pt 'Franklin Gothic Medium';
    padding-left: 10px;
    padding-right: 10px;
}
#btn_translate_output{
    border: 1px solid #7a7a7a;
    border-radius: 0px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
#grp_translate_googleTranslate{
    font: bold 10pt 'Franklin Gothic Medium';
    text-decoration: underline;
}

/* Translate Form Translate */
#pgr_translate_translate{
    margin-bottom: -20px;
}
#lbl_translate_status{
    font: italic Arial, Helvetica, sans-serif;
    margin-top: -20px;
